p: 1.5rem;\n    background-color: hsl(228, 56%, 26%);\n\n}\n\n.offer {\n    border-top-right-radius: 6rem;\n    padding-bottom: .5rem;\n}\n\n.offer__logo {\n    /* width: 60%; */\n    object-fit: cover;\n}\n\n.offer-container {\n    padding: 2rem;\n    padding-left: 0;\n    display: grid;\n    grid-auto-flow: column;\n    justify-content: start;\n    column-gap: 1rem;\n}\n\n.offer-container__icon {\n    /* border: 1px solid green; */\n    padding: 1rem;\n    border-radius: .7rem;\n    display: grid;\n    justify-content: center;\n    align-content: center;\n    width: 3.2rem;\n    height: 3.2rem;\n    background-color: hsl(229, 57%, 11%);\n\n}\n\n.storage {\n    padding-left: 1.8rem;\n    padding-right: 1.8rem;\n    position: relative;\n}\n\n.storage__text {\n    font-size: calc(.2rem + 3vw);\n    color: hsl(243, 100%, 93%);\n    text-align: center;\n    margin-bottom: 1.3rem;\n    \n}\n\n.progressbar {\n    background-color: #15204b;\n    border-radius: 1rem;\n  /* (height of inner div) / 2 + padding */\n    padding: 3px;\n    display: grid;\n    grid-auto-flow: column;\n}\n\n.progressbar > div {\n    background: rgb(255,163,153);\n    background: -moz-linear-gradient(90deg, rgba(255,163,153,1) 0%, rgba(255,77,151,1) 100%);\n    background: -webkit-linear-gradient(90deg, rgba(255,163,153,1) 0%, rgba(255,77,151,1) 100%);\n    background: linear-gradient(90deg, rgba(255,163,153,1) 0%, rgba(255,77,151,1) 100%);\n    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=\"#ffa399\",endColorstr=\"#ff4d97\",GradientType=1); \n    width: 80%;\n  /* Adjust with JavaScript */\n    height: .8rem;\n    border-radius: 10px;\n    display: flex;\n    justify-content: flex-end;\n    align-items: center;\n}\n\n.dot {\n  height: .7rem;\n  width: .7rem;\n  background-color: white;\n  border-radius: 50%;\n  display: inline-block;\n  margin-right: .1rem;\n}\n\n.storage__progress__report {\n    display: flex;\n    justify-content: space-between;\n    padding-top: .8rem;\n    padding-bottom: 1rem;\n    /* font-size: calc(.2rem + 2.5vw); */\n    color: hsl(243, 100%, 93%);\n}\n\n.storage__data-remaining {\n    background-color: white;\n    color: hsl(229, 57%, 11%);\n    font-size: 2.5rem;\n    padding: 1rem;\n    width: 60%;\n    text-align: center;\n    margin: auto;\n    position: absolute;\n    padding-left: 0;\n    padding-right: 0;\n    border-radius: .5rem;\n    /* left: 50%; */\n    /* top: 0; */\n    left: 0;\n    bottom: -2rem;\n    right: 0;\n}\n\n.storage__data-remaining--grey {\n    color: grey;\n    font-size: 1rem;\n    vertical-align: middle;\n}"},"lineCount":null}},"error":null,"hash":"b314465c7c04053c41081b1bfca015aa","cacheData":{"env":{}}}